  <xs:simpleType name="version_num">
    <xs:restriction base="xs:string">
      <xs:pattern value="(0|[1-9]([0-9]*))\.(0|[1-9]([0-9]*))"/>
    </xs:restriction>
  </xs:simpleType>
  <!-- A valid version number M.N or *.* or combination of both. -->
  <xs:simpleType name="version_num_ref">
    <xs:restriction base="xs:string">
      <xs:pattern value="((0|\*|[1-9]([0-9]*)))\.((0|\*|[1-9]([0-9]*)))"/>
    </xs:restriction>
  </xs:simpleType>
</xs:schema>
